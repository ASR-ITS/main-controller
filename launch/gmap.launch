<launch>
  <!-- Gmapping -->
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmap" output="screen">
    <param name="base_frame" value="base_link"/>
    <param name="odom_frame" value="odom"/>
    <param name="map_frame"  value="map"/>
    <rosparam command="load" file="$(find asr_its)/config/gmapping_params.yaml" />
  </node>
</launch>